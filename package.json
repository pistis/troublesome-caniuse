{
  "name": "troublesome-caniuse",
  "version": "0.0.1",
  "description": "Browser native technology compatibility report using caniuse db",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pistis/troublesome-caniuse.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [
    "caniuse",
    "report",
    "compatibility"
  ],
  "author": {
    "name": "pistis",
    "email": "vamalboro@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pistis/troublesome-caniuse/issues"
  },
  "homepage": "https://github.com/pistis/troublesome-caniuse#readme",
  "dependencies": {
    "eslint": "^5.16.0"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "prettier": "1.18.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/.report/jest/",
    "coverageReporters": [
      "html",
      "text",
      "json"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!**/node_modules/**"
    ],
    "verbose": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/spec/*.js?(x)"
    ]
  }
}
